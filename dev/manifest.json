{
  "manifest_version": 3,
  "name": "ChatGPT Prompt Sender",
  "version": "1.0",
  "description": "Send a prompt to ChatGPT and get a response.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
